<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/question_text_view2"
        android:layout_width="244dp"
        android:layout_height="105dp"
        android:text="@string/question_australia"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="90dp" />

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </FrameLayout>

    <Button
        android:id="@+id/mTrueButton2"
        android:layout_width="252dp"
        android:layout_height="74dp"
        android:layout_marginStart="268dp"
        android:layout_marginLeft="268dp"
        android:text="@string/true_button"
        app:layout_constraintStart_toStartOf="@+id/mFalseButton2"
        tools:layout_editor_absoluteY="219dp" />

    <Button
        android:id="@+id/mFalseButton2"
        android:layout_width="249dp"
        android:layout_height="74dp"
        android:text="@string/false_button"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="219dp" />

    <Button
        android:id="@+id/mNextButton2"
        android:layout_width="248dp"
        android:layout_height="67dp"
        android:drawableEnd="@drawable/arrow_right"
        android:text="@string/next_button"
        tools:layout_editor_absoluteX="458dp"
        tools:layout_editor_absoluteY="328dp" />

</androidx.constraintlayout.widget.ConstraintLayout>